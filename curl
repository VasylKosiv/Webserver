curl -X POST -s localhost:8080/api/v1/list -d '{"FirstName":"FIRSTNAME", "LastName":"LASTNAME", "Age":18}'
curl -X GET -s localhost:8080/api/v1/list
curl -X GET -s localhost:8080/api/v1/list/REAL ID
curl -X PUT -s localhost:8080/api/v1/list/REAL ID -d '{"FirstName":"NAME","LastName":"SURNAME","Age":99}'
curl -X DELETE -s localhost:8080/api/v1/list/REAL ID
